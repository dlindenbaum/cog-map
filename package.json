{
  "name": "ol-parceljs",
  "version": "1.0.0",
  "description": "Example using OpenLayers with Parcel",
  "main": "main.js",
  "dependencies": {
    "jquery": "^3.2.1",
    "ol": "^4.6.4",
    "ol-hashed": "^1.0.2",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
